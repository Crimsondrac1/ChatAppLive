<h2>Login</h2>
<form class="login-form">
  <div>
    <label for="email-login">email:</label>
    <input type="text" id="email-login" class="textbox-style" />
  </div>
  <div>
    <label for="password-login">password:</label>
    <input type="password" id="password-login" class="textbox-style" />
  </div>
  <div>
    <button type="submit" class="userButton">login</button>
    <a href="/signup" class="ml-5 newUser">Create a new account</a>
  </div>  
</form>

<script src="/js/login.js"></script>
